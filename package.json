{
  "name": "@oada/oada-lookup",
  "version": "1.0.0",
  "description": "JavaScript utility library to lookup OADA documents such as Well-Known (RFC 5785) resource, e.g., oada-configuration, openid-configuration, etc, and public OADA client registrations",
  "keywords": [
    "OADA",
    "oada-configuration",
    "openid-configuration",
    "client registration",
    "client discovery"
  ],
  "homepage": "https://github.com/OADA/oada-lookup-js",
  "bugs": {
    "url": "https://github.com/OADA/oada-lookup-js/issues"
  },
  "license": "Apache-2.0",
  "author": "Andrew Balmos <abalmos@purdue.edu>",
  "main": "lookup.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OADA/oada-lookup-js.git"
  },
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "test": "_mocha -w",
    "fix": "prettier-standard",
    "lint": "prettier-standard --lint",
    "cover": "istanbul cover mocha -- -R spec",
    "clean": "rm -rf ./coverage"
  },
  "mocha": {
    "exit": true,
    "reporter": "spec",
    "ui": "bdd",
    "timeout": 200
  },
  "dependencies": {
    "oada-error": "^1.1.1",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "husky": "^4.2.3",
    "istanbul": "^0.4.5",
    "lint-staged": "^10.0.7",
    "mocha": "^7.0.1",
    "nock": "^12.0.1",
    "prettier-standard": "^16.1.0"
  },
  "directories": {
    "test": "test"
  }
}
